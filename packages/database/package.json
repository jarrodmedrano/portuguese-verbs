{
  "name": "database",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.js",
  "dependencies": {
    "@prisma/client": "5.1.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/node": "17.0.21",
    "csv-parse": "5.3.3",
    "prisma": "5.1.1",
    "ts-node-dev": "1.1.8",
    "typescript": "5.1.6"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "datamodel": "prisma/datamodel.prisma",
    "client": "prisma/client",
    "seed": "npx ts-node prisma/seed.ts"
  },
  "scripts": {
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "migrate dev": "npx prisma migrate dev --create-only --name init",
    "migrate": "npx prisma migrate dev",
    "prebuild": "ls -a",
    "build": "npx pnpm run generate && npx pnpm run migrate && npx pnpm run db:seed && tsc -p tsconfig.json",
    "generate": "npx prisma generate",
    "dev": "npx pnpm run build && tsnd --respawn src/index.ts",
    "db:seed": "npx prisma db seed"
  }
}
