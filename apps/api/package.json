{
  "name": "api",
  "version": "1.0.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "MIT",
  "dependencies": {
    "@trpc/server": "9.20.0",
    "axios": "1.2.6",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.17.3",
    "zod": "3.20.2",
    "prisma": "5.1.1",
    "@prisma/client": "5.1.1",
    "dotenv-expand": "10.0.0",
    "openai": "3.1.0"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "datamodel": "prisma/datamodel.prisma",
    "client": "prisma/client",
    "seed": "npx ts-node prisma/seed.ts"
  },
  "scripts": {
    "migrate dev": "npx prisma migrate dev --create-only --name init",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "dev": "npx pnpm run prebuild && tsnd --respawn src/index.ts",
    "db:seed": "npx prisma db seed",
    "prebuild": "npx pnpm run generate && npx pnpm run migrate && npx pnpm run db:seed",
    "build": "tsc -p tsconfig.json"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/node": "17.0.21",
    "csv-parse": "5.3.3",
    "ts-node-dev": "1.1.8",
    "typescript": "5.1.6"
  }
}
